{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "require": true,
    "module": true,
    "$": true,
    "browser": true,
    "THREE": true
  },
  "rules": {
    "no-console": 0,
    "semi": 0,
    "arrow-parens": [2, "as-needed"],
    "no-underscore-dangle": 0,
    "one-var": 0,
    "padded-blocks": 0,
    "max-len": ["warn", 120],
    "global-require": 0,
    "prefer-const": 0,
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-template": 1,
    "no-param-reassign": 1,
    "no-plusplus": 0,
    "eqeqeq":[1,"smart"],
    "import/prefer-default-export": 0,
    "indent": [
        "error",
        4
    ]
  }
}
